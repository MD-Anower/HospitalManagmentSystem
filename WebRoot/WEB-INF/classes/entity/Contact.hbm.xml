<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
    
<hibernate-mapping>
       <class name="entity.Contact" table="Contact">
          <id name="Id" column="Id">
          <generator class="native"></generator>
          </id>
          <property name="fullName" column="FullName" type="string"></property>
          <property name="email" column="Email" type="string"></property>
          <property name="phone" column="Phone" type="string"></property>
          <property name="message" column="Message" type="string"></property>
         
       </class>
</hibernate-mapping> 